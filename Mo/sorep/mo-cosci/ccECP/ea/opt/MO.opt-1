include Mo.sys

trialfunc { include Mo.opt.wfout }

method { vmc
	nconfig 10
	nblock 20
	timestep 0.47
	nstep 30
        dynamics {
        dynspin
        spin_mass 10
        }
	storeconfig Mo.vmc_opt1.config
 }

method { optimize
	nconfig 50
	nstep 50
	ITERATIONS 30
	readconfig Mo.vmc_opt1.config	
	wfoutput Mo.opt1.wfout
 }
